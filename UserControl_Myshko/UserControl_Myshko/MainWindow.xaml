<Window xmlns:MaskTextBox="clr-namespace:MaskTextBox;assembly=MaskTextBox" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserControl_Myshko"
        xmlns:WpfControlLibrary1="clr-namespace:WpfControlLibrary1;assembly=WpfControlLibrary1" x:Class="UserControl_Myshko.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="230" Width="260" MinWidth="260" MinHeight="230" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="76*"/>
            <RowDefinition Height="113*"/>
        </Grid.RowDefinitions>

        <WpfControlLibrary1:UserControl1 x:Name="TableButton" Margin="5" Grid.Row="1" NumClick="TableButton_NumClick" BackClick="TableButton_BackClick" OkayClick="TableButton_OkayClick" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <MaskTextBox:NewTextBox x:Name="textBox" Mask="###-###-###" CharReplace="#" VerticalAlignment="Center" Margin="15" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>

    </Grid>
</Window>
